#################################################################
#
#    Object Properties
#
#################################################################


###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#pathwayComponent

:pathwayComponent rdf:type :ObjectProperty ;
                  
                  rdfs:comment "Represents a isub-pathway within a parent pathway." ;
                  
                  rdfs:domain :Pathway ;
                  
                  rdfs:range :Pathway .



###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#publication

:publication rdf:type :ObjectProperty ;
             
             rdfs:comment "Publication associated with the pathway or experiment." ;
             
             rdfs:range :Publication ;
             
             rdfs:domain [ rdf:type :Class ;
                           :unionOf ( :Experiment
                                      :Pathway
                                    )
                         ] .



###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#species

:species rdf:type :ObjectProperty ;
         
         rdfs:comment "The species the expreiment was run on." ;
         
         rdfs:range :Species ;
         
         rdfs:domain [ rdf:type :Class ;
                       :unionOf ( :Experiment
                                  :Pathway
                                )
                     ] .





#################################################################
#
#    Data properties
#
#################################################################


###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#author

:author rdf:type :DatatypeProperty ,
                 :FunctionalProperty ;
        
        rdfs:comment "The authors of this publication, one per property value."^^xsd:string ;
        
        rdfs:range xsd:string ;
        
        rdfs:domain [ rdf:type :Class ;
                      :unionOf ( :Experiment
                                 :Pathway
                                 :Publication
                               )
                    ] .



###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#description

:description rdf:type :DatatypeProperty ;
             
             rdfs:comment "Experiment description" ;
             
             rdfs:domain :Experiment ;
             
             rdfs:range xsd:string .



###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#id

:id rdf:type :DatatypeProperty ,
             :FunctionalProperty ;
    
    rdfs:comment "Unique identifier"^^xsd:string ;
    
    rdfs:range xsd:string ;
    
    rdfs:domain [ rdf:type :Class ;
                  :unionOf ( :Experiment
                             :Pathway
                             :Publication
                           )
                ] .



###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#name

:name rdf:type :DatatypeProperty ;
      
      rdfs:comment "Name of entity"^^xsd:string ;
      
      rdfs:range xsd:string ;
      
      rdfs:domain [ rdf:type :Class ;
                    :unionOf ( :Experiment
                               :Pathway
                               :Publication
                             )
                  ] .



###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#title

:title rdf:type :DatatypeProperty ,
                :FunctionalProperty ;
       
       rdfs:comment "The title of the publication." ;
       
       rdfs:domain :Publication ;
       
       rdfs:range xsd:string .



###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#year

:year rdf:type :DatatypeProperty ;
      
      rdfs:range xsd:string ;
      
      rdfs:domain [ rdf:type :Class ;
                    :unionOf ( :Experiment
                               :Pathway
                               :Publication
                             )
                  ] .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#Experiment

:Experiment rdf:type :Class .



###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#ExperimentWithPublication

:ExperimentWithPublication rdf:type :Class ;
                           
                           :equivalentClass [ rdf:type :Class ;
                                              :intersectionOf ( :Experiment
                                                                [ rdf:type :Restriction ;
                                                                  :onProperty :publication ;
                                                                  :onClass :Publication ;
                                                                  :minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                                ]
                                                              )
                                            ] .



###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#Pathway

:Pathway rdf:type :Class .



###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#PathwayWithPublication

:PathwayWithPublication rdf:type :Class ;
                        
                        :equivalentClass [ rdf:type :Class ;
                                           :intersectionOf ( :Pathway
                                                             [ rdf:type :Restriction ;
                                                               :onProperty :publication ;
                                                               :onClass :Publication ;
                                                               :minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                             ]
                                                           )
                                         ] .



###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#Publication

:Publication rdf:type :Class .



###  http://www.semanticweb.org/ryan/ontologies/2015/2/untitled-ontology-24#Species

:Species rdf:type :Class .





